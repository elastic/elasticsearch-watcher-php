{
  "name": "elasticsearch/watcher",
  "description": "Watcher namespace for Elasticsearch-PHP",
  "keywords": ["search","client", "elasticsearch", "alerts", "watcher"],
  "type": "library",
  "license": "Apache 2",
  "authors": [
    {
      "name": "Zachary Tong"
    }
  ],
  "require": {
    "php": ">=5.3.9",
    "elasticsearch/elasticsearch": "dev-master"
  },
  "require-dev": {
      "cpliakas/git-wrapper": "~1.0",
      "phpunit/phpunit": "3.7.*",
      "symfony/yaml": "2.4.3 as 2.4.2"
  },
  "autoload": {
    "psr-4": {
      "Watcher\\": "src/Watcher/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Watcher\\Tests\\":      "tests/Watcher/Tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/polyfractal/Yaml"
    }
  ]
}
